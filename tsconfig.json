{
    "compilerOptions": {
      "target": "es5",
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
       "jsx": "react-jsx",
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noImplicitThis": false,
      "incremental": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"],
        "~/*": ["./public/*"],
        "#/*": ["./*"]
      },
    },
    "exclude": ["node_modules", "public"]
}
  